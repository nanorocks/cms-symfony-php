{# templates/layouts/base.html.twig #}
<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Your CMS - Empowering Content Management{% endblock %}</title>
    {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('dist/css/styles.css') }}">
    {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}

    {% block javascripts %}
    {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>

<body class="bg-gray-100 font-sans">
    {% block aside %}
    {% include './admin/components/aside.html.twig' %}
    {% endblock %}
    {% block header %}
    {% include './admin/components/header.html.twig' %}
    {% endblock %}
    {% block body %}
    {% block content %}
    {% endblock %}
    {% endblock %}
    {% block footer %}
    {% include './admin/components/footer.html.twig' %}
    {% endblock %}
</body>
</html>
